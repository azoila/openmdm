{
  "name": "@openmdm/storage-s3",
  "version": "0.2.0",
  "description": "S3 storage adapter for OpenMDM - APK uploads with presigned URLs",
  "author": "OpenMDM Contributors",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.5.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-s3": ">=3.0.0"
  },
  "keywords": [
    "openmdm",
    "s3",
    "storage",
    "apk",
    "upload"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azoila/openmdm.git",
    "directory": "packages/storage/s3"
  },
  "homepage": "https://openmdm.dev",
  "bugs": {
    "url": "https://github.com/azoila/openmdm/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
