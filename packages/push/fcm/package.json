{
  "name": "@openmdm/push-fcm",
  "version": "0.1.0",
  "description": "Firebase Cloud Messaging push adapter for OpenMDM",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@openmdm/core": "workspace:*",
    "firebase-admin": "^13.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.5.0"
  },
  "peerDependencies": {
    "firebase-admin": ">=12.0.0"
  },
  "keywords": [
    "openmdm",
    "fcm",
    "firebase",
    "push",
    "notifications"
  ],
  "license": "MIT"
}
