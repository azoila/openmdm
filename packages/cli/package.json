{
  "name": "@openmdm/cli",
  "version": "0.2.1",
  "description": "CLI tools for OpenMDM - database migrations, admin commands, and device management",
  "author": "OpenMDM Contributors",
  "type": "module",
  "bin": {
    "openmdm": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@openmdm/core": "workspace:*",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.0",
    "ora": "^8.0.0",
    "dotenv": "^16.0.0",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/qrcode": "^1.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0"
  },
  "keywords": [
    "openmdm",
    "cli",
    "mdm",
    "admin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azoila/openmdm.git",
    "directory": "packages/cli"
  },
  "homepage": "https://openmdm.dev",
  "bugs": {
    "url": "https://github.com/azoila/openmdm/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
